  "scripts": {
    "start:server": "node ./src/server/index.js",
    "start:dev": "npm run build:dev && cross-env NODE_ENV=dev node ./src/server/index.js",
    "start:pro": "npm run build:pro && cross-env NODE_ENV=production node ./src/server/index.js",
    "start:client": "webpack-dev-server --inline --content-base . --history-api-fallback --host 0.0.0.0 --port 9800",
    "start:clientdev": "cross-env NODE_ENV=dev webpack-dev-server --inline --content-base . --history-api-fallback --host 0.0.0.0 --port 9800",
