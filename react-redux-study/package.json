    "build:client": "webpack --colors --profile --display-modules",
    "build:dev": "cross-env NODE_ENV=dev webpack",
    "build:pro": "cross-env NODE_ENV=production webpack --colors --profile --display-modules",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js"
  },
