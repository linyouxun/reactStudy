{
  "name": "study",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:client": "webpack-dev-server --inline --content-base . --history-api-fallback --host 0.0.0.0 --port 9800 --colors --profile --display-modules",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server.bundle.js",
    "start:dev": "npm run build && cross-env NODE_ENV=dev node server.bundle.js",
    "build:client": "webpack --colors --profile --display-modules",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js"
  },
  "author": "lyx",
  "license": "ISC",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-core": "~6.2.1",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-react": "~6.1.18",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.16.2"
  }
}
